<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>cadastroEvento</from-outcome>
			<to-view-id>cadastroEvento.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>telaLogin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>alterarSenha</from-outcome>
			<to-view-id>alterarSenha.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastroPalestrante</from-outcome>
			<to-view-id>cadastroConvidado.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastrarUsuario</from-outcome>
			<to-view-id>cadastroUsuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>principal</from-outcome>
			<to-view-id>index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarEventos</from-outcome>
			<to-view-id>listagemEventos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarPalestrantes</from-outcome>
			<to-view-id>listagemConvidados.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarUsuarios</from-outcome>
			<to-view-id>listagemUsuarios.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>confirmarPalestrante</from-outcome>
			<to-view-id>confirmacaoConvidado.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>telaLogin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>alterarSenha</from-outcome>
			<to-view-id>alterarSenha.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>mapa</from-outcome>
			<to-view-id>mapaInterface.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastrarAlerta</from-outcome>
			<to-view-id>cadastroAlerta.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarAlerta</from-outcome>
			<to-view-id>listagemAlertas.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastrarPostagem</from-outcome>
			<to-view-id>cadastroPostagem.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarPostagem</from-outcome>
			<to-view-id>listagemPostagens.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastrarVideo</from-outcome>
			<to-view-id>cadastroVideo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarVideo</from-outcome>
			<to-view-id>listagemVideos.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>cadastrarParceiro</from-outcome>
			<to-view-id>cadastroParceiro.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarParceiro</from-outcome>
			<to-view-id>listagemParceiros.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>confimarParceiro</from-outcome>
			<to-view-id>confirmacaoParceiro.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>relatorioParticipantes</from-outcome>
			<to-view-id>participantesInterface.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<from-view-id>cadastroEvento.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>confirmar</from-outcome>
			<to-view-id>confirmacaoEvento.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
